{{>header}}

<div class="row center-align">
    <h3>List Comments of Issue: {{issueId}} of {{username}}</h3>
</div>
{{#comments}}
<div class="row">
    <div class="col s6">
        <div class="card">
            <div class="card-content">
                <span class="card-title">{{id}}</span>
                <p>{{descrizione}}</p>
                <p>{{author}}</p>
            </div>
        </div>
    </div>
</div>
{{/comments}}
{{>footer}}