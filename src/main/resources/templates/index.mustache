{{>header}}

<div class="row center-align">
    <h3>List projects of: {{username}}</h3>
</div>
<div class="row">
    <div class="col s6">
        <div class="card">
            <div class="card-content">
                <span class="card-title">AA</span>
                <p>BB</p>
            </div>
            <div class="card-action">
                <form name="movie" action="/vote" method="post">
                    <input type="hidden" name="id" value="1">
                    <button class="btn waves-effect waves-light" type="submit" name="action">Vote
                    </button>
                </form>
            </div>
        </div>
    </div>
</div>

{{>footer}}